                local endTime = tick() + 3
                while tick() < endTime do
                    hrp.CFrame = CFrame.new(safeZonePosition)
                    RunService.RenderStepped:Wait()
                end
            end
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "âœ… ØªÙ… Ø§Ù„Ø¥ÙŠÙ‚Ø§Ù",
