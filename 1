                Title = "âš”ï¸ Ø§Ù„Ù†Ø¸Ø§Ù… Ù…ÙØ¹Ù„",
                Text = "ÙŠØªÙ… Ø§Ù„Ø¢Ù† Ø§Ù„Ø§Ù†ØªÙ‚Ø§Ù„ ÙˆØ§Ù„ØªØ¬Ù‡ÙŠØ² ÙˆØ§Ù„Ù‚ØªÙ„ Ø§Ù„ØªÙ„Ù‚Ø§Ø¦ÙŠ.",
                Duration = 3
